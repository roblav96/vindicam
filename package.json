{
	"name": "vindicam",
	"version": "0.0.1",
	"description": "Vindicate yourself with a live video recording saved to remote storage",
	"nativescript": {
		"id": "app.vindicam",
		"tns-android": {
			"version": "6.5.3"
		},
		"tns-ios": {
			"version": "6.5.2"
		}
	},
	"scripts": {
		"au": "adb uninstall app.vindicam || true",
		"iu": "ideviceinstaller --uninstall app.vindicam || true",
		"xcode": "tns prepare ios && open platforms/ios/vindicam.xcworkspace",
		"del": "del hooks node_modules platforms",
		"delp": "del platforms",
		"postinstall": "run-p postinstall:*",
		"postinstall:update-ns-webpack": "update-ns-webpack --configs"
	},
	"dependencies": {
		"@nativescript/core": "^6.5.12",
		"@nativescript/theme": "^2.3.3",
		"lodash-es": "^4.17.15",
		"nativescript-android-utils": "^1.0.2",
		"nativescript-permissions": "^1.3.9",
		"nativescript-videorecorder": "^3.0.0-beta.6",
		"rambda": "^5.9.0",
		"rambdax": "^3.7.0",
		"shimmer": "^1.2.1",
		"string-fn": "^2.12.0",
		"tns-core-modules": "^6.5.12"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.158",
		"@types/shimmer": "^1.0.1",
		"del-cli": "^3.0.1",
		"nativescript-dev-webpack": "^1.5.1",
		"npm-run-all": "^4.1.5",
		"tns-platform-declarations": "^6.5.12",
		"typescript": "^3.9.7"
	},
	"license": "Unlicense",
	"author": "Robert Laverty <roblav96@gmail.com> (https://github.com/roblav96)",
	"homepage": "https://github.com/roblav96/vindicam#readme",
	"repository": "github:roblav96/vindicam",
	"bugs": "https://github.com/roblav96/vindicam/issues"
}
