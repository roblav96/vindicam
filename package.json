{
	"name": "vindicam",
	"version": "0.0.1",
	"description": "Vindicate yourself with a live video recording saved to remote storage",
	"main": "app.js",
	"nativescript": {
		"id": "app.vindicam",
		"tns-android": {
			"version": "6.5.1"
		},
		"tns-ios": {
			"version": "6.5.1"
		}
	},
	"scripts": {
		"au": "adb uninstall app.vindicam || true",
		"iu": "ideviceinstaller --uninstall app.vindicam || true",
		"xcode": "tns prepare ios && open platforms/ios/vindicam.xcworkspace",
		"del": "del hooks node_modules platforms",
		"delp": "del platforms",
		"postinstall": "update-ns-webpack --configs && run-p postinstall:*",
		"postinstall:webpack.alias": "sd -s \"    alias['~'] = appFullPath;\" \"    alias['@'] = appFullPath; alias['~'] = appFullPath;\" webpack.config.js",
		"update-ns-webpack": "update-ns-webpack --configs"
	},
	"dependencies": {
		"@nativescript/core": "^6.5.8",
		"@nativescript/theme": "^2.3.3",
		"lodash-es": "^4.17.15",
		"nativescript-webrtc-plugin": "^2.0.0-alpha.22",
		"rambda": "^5.8.0",
		"rambdax": "^3.7.0",
		"shimmer": "^1.2.1",
		"tns-core-modules": "^6.5.8"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.157",
		"@types/shimmer": "^1.0.1",
		"del-cli": "^3.0.1",
		"nativescript-dev-webpack": "^1.5.1",
		"npm-run-all": "^4.1.5",
		"tns-platform-declarations": "^6.5.8",
		"typescript": "^3.9.5"
	},
	"license": "Unlicense",
	"author": "Robert Laverty <roblav96@gmail.com> (https://github.com/roblav96)",
	"homepage": "https://github.com/roblav96/vindicam#readme",
	"repository": "github:roblav96/vindicam",
	"bugs": "https://github.com/roblav96/vindicam/issues"
}
